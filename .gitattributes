# data stored in textual formats
*_data.csv filter=lfs diff=lfs merge=lfs -text
*_data.tsv filter=lfs diff=lfs merge=lfs -text
*_data.json filter=lfs diff=lfs merge=lfs -text
*_data.ndjson filter=lfs diff=lfs merge=lfs -text

# images
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
